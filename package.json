{
  "name": "@newhighsco/circleci-orb",
  "description": "New High Score shareable config for CircleCI",
  "version": "1.0.3",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/circleci-orb.git"
  },
  "homepage": "https://github.com/newhighsco/circleci-orb#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/circleci-orb/issues"
  },
  "files": [],
  "scripts": {
    "prepare": "brew bundle",
    "prebuild": "rm -rf packed-orb.yml",
    "build": "circleci orb pack --skip-update-check src > packed-orb.yml",
    "pretest": "yarn lint",
    "test": "circleci orb validate --skip-update-check packed-orb.yml",
    "lint": "yamllint ."
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@newhighsco/commitlint-config": "1.0.18",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/release-config": "1.1.2",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "semantic-release": "18.0.1"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "release": {
    "extends": "@newhighsco/release-config/orb",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  }
}
